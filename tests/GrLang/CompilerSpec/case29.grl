import "vcs-types.grl"

graph g {
  r : Revision
  d : Deps
  r -e1 e2:MDeps-> d
}

graph h {
  rr : Revision
  dd : Deps
  rr -e:MDeps-> dd
}

morphism f1 {
  domain g
  domain g
  codomain h

  r -> rr
  d -> dd
  e1 e2 -> e
}

morphism f2 {
  domain g
  codomain h
  codomain h

  r -> rr
  d -> dd
  e1 e2 -> e
}